{
  "name": "d2l-component-catalog",
  "description": "",
  "scripts": {
    "build": "npm run clean && node ./tools/generate-pages.mjs && npm run build:rollup",
    "build:prod": "cross-env NODE_ENV=production npm run build && cross-env NODE_ENV=production npx eleventy",
    "build:rollup": "rollup -c rollup.config.js",
    "clean": "rimraf _site && rimraf .generated && rimraf pages/components/imported",
    "lint": "npm run lint:eslint && npm run lint:lit && npm run lint:style",
    "lint:eslint": "eslint . --ext .js,.mjs",
    "lint:lit": "lit-analyzer pages/**/*.js --strict",
    "lint:style": "stylelint \"**/*.js\"",
    "start": "cross-env NODE_ENV=development npm run build && eleventy && concurrently \"npx eleventy --watch\" \"web-dev-server\"",
    "start:prod": "npm run build:prod && web-dev-server --config web-dev-server-prod.config.js",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "mocha"
  },
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "@11ty/eleventy": "^0.12",
    "@11ty/eleventy-navigation": "^0.2",
    "@babel/core": "^7",
    "@babel/eslint-parser": "^7",
    "@babel/preset-env": "^7",
    "@brightspace-ui/core": "^1",
    "@brightspace-ui/stylelint-config": "^0.2",
    "@octokit/rest": "^18",
    "@open-wc/building-rollup": "^1",
    "@web/dev-server": "^0.1",
    "clean-css": "^5",
    "concurrently": "^6",
    "cross-env": "^7",
    "deepmerge": "^4",
    "eslint": "^7",
    "eslint-config-brightspace": "^0.14",
    "eslint-plugin-html": "^6",
    "eslint-plugin-lit": "^1",
    "eslint-plugin-sort-class-members": "^1",
    "gray-matter": "^4",
    "lit-analyzer": "^1",
    "markdown-it-modify-token": "^1",
    "mocha": "^8",
    "rimraf": "^3",
    "rollup": "^2",
    "rollup-plugin-output-manifest": "^2",
    "sinon": "^11",
    "stylelint": "^13"
  }
}
